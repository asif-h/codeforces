1409A- Yet another two integer problem


#include<iostream>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		long long int a,b,count,temp,rem;
		cin>>a>>b;
		temp=b-a;
		if(temp<0)
		    temp=-temp;
		count=temp/10;
		rem=temp%10;
		if(rem>0)
		   count++;
		cout<<count<<endl;
	}
	return 0;
}
